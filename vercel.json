{
  "name": "comment",
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "index.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
